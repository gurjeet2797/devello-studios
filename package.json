{
  "name": "devello-inc",
  "description": "Turn your sketch into a refined image using AI",
  "appName": "Devello Inc",
  "appSubtitle": "Turn your sketch into a refined image using AI",
  "appMetaDescription": "Turn your sketch into a refined image using AI",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -H 0.0.0.0 -p 3001",
    "dev:studios": "next dev -H 0.0.0.0 -p 3001",
    "build": "next build",
    "build:fast": "SKIP_ENV_VALIDATION=true next build",
    "start": "next start",
    "lint": "next lint",
    "production-build": "NODE_ENV=production npm run build",
    "debug-mode": "DEBUG_MODE=true npm run dev",
    "security-check": "npm audit --audit-level moderate",
    "clean": "powershell -ExecutionPolicy Bypass -File scripts/clean-build.ps1",
    "test": "npm run lint && npm run build",
    "test-supabase": "node scripts/test-supabase.js",
    "test-spam": "node scripts/test-spam-protection.js",
    "dev-reset": "node scripts/dev-reset.js",
    "postinstall": "prisma generate"
  },
  "browser": {
    "@prisma/client": false,
    "prisma": false,
    "sharp": false
  },
