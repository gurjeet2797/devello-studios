# Production Environment Variables Template
# Copy this to .env.production and fill in your actual values

# Database
POSTGRES_PRISMA_URL=postgresql://username:password@host:port/database?schema=public

# Supabase (if using)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_ANON_KEY=your-anon-key

# AI Services
REPLICATE_API_TOKEN=r8_your-replicate-token
GOOGLE_API_KEY=your-google-api-key

# Stripe (if using payments)
STRIPE_PUBLISHABLE_KEY=pk_live_your-stripe-key
STRIPE_SECRET_KEY=sk_live_your-stripe-secret
STRIPE_WEBHOOK_SECRET=whsec_your-webhook-secret

# Security
# Note: This application uses Supabase Auth, not NextAuth
# No NextAuth configuration is required

# Production Settings
NODE_ENV=production
LOG_LEVEL=error
DEBUG_MODE=false

# Optional: Performance monitoring
SENTRY_DSN=your-sentry-dsn
VERCEL_ANALYTICS_ID=your-analytics-id
